<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE robot SYSTEM "robot.dtd">
<robot>
     <pdp canId="0"
          type="CTRE"/>
     <pigeon canId="0"
             usage="CENTER_OF_ROTATION"
             type="pigeon1"
             rotation="0.0"/>
     <roborio orientation="X_FORWARD_Y_LEFT"/>

 <!-- ====================================================
     Chassis
     ====================================================  -->
<!-- -->
       <chassis  type="MECANUM"
              wheelDiameter="4.0"
              wheelBase="21.0"  
              track="21.0"
              maxVelocity="1.0"
              maxAngularVelocity="1"
              maxAcceleration="1"
              maxAngularAcceleration="1"
              networkTable="ChassisNT"
              controlFile="chassis.xml">
			 
            <motor  usage="MECANUM_LEFT_FRONT"
                    canId="7"
                    pdpID="7"
                    type="TALONSRX"
                    inverted="true"
                    sensorInverted="false"
                    feedbackDevice="NONE"
                    countsPerRev="1"
                    gearRatio="1"
                    brakeMode="true"
                    follow="-1"
                    voltageCompensationSaturation="10"
                    voltageCompensationEnable="true"
                    peakCurrentDuration="25.0"
                    continuousCurrentLimit="20.0"
                    peakCurrentLimit="25.0"
                    currentLimiting="true" 
                    forwardlimitswitch="false"
                    forwardlimitswitchopen="true"
                    reverselimitswitch="false"
                    reverselimitswitchopen="true"/>                  
             <motor  usage="MECANUM_LEFT_BACK"
                    canId="5"
                    pdpID="5"
                    type="TALONSRX"
                    inverted="true"
                    sensorInverted="false"
                    feedbackDevice="NONE"
                    countsPerRev="0"
                    gearRatio="1.0"
                    brakeMode="true"
                    follow="-1"
                    voltageCompensationSaturation="10"
                    voltageCompensationEnable="true"
                    peakCurrentDuration="25.0"
                    continuousCurrentLimit="20.0"
                    peakCurrentLimit="25.0"
                    currentLimiting="true" 
                    forwardlimitswitch="false"
                    forwardlimitswitchopen="true"
                    reverselimitswitch="false"
                    reverselimitswitchopen="true"/>        
  			 
            <motor  usage="MECANUM_RIGHT_FRONT"
                    canId="8"
                    pdpID="8"
                    type="TALONSRX"
                    inverted="false"
                    sensorInverted="false"
                    feedbackDevice="NONE"
                    countsPerRev="1"
                    gearRatio="1"
                    brakeMode="true"
                    follow="-1"
                    voltageCompensationSaturation="10"
                    voltageCompensationEnable="true"
                    peakCurrentDuration="25.0"
                    continuousCurrentLimit="20.0"
                    peakCurrentLimit="25.0"
                    currentLimiting="true" 
                    forwardlimitswitch="false"
                    forwardlimitswitchopen="true"
                    reverselimitswitch="false"
                    reverselimitswitchopen="true"/>                  
             <motor  usage="MECANUM_RIGHT_BACK"
                    canId="9"
                    pdpID="9"
                    type="TALONSRX"
                    inverted="false"
                    sensorInverted="false"
                    feedbackDevice="NONE"
                    countsPerRev="1"
                    gearRatio="1.0"
                    brakeMode="true"
                    follow="-1"
                    voltageCompensationSaturation="10"
                    voltageCompensationEnable="true"
                    peakCurrentDuration="25.0"
                    continuousCurrentLimit="20.0"
                    peakCurrentLimit="25.0"
                    currentLimiting="true" 
                    forwardlimitswitch="false"
                    forwardlimitswitchopen="true"
                    reverselimitswitch="false"
                    reverselimitswitchopen="true"/>        
       </chassis>  

<!-- ====================================================
     Intake
     ====================================================  -->
     <mechanism type="INTAKE" 
               networkTable="IntakeNT" 
               controlFile="intake.xml"> 
          <motor usage="INTAKE" 
                 canId="6" 
                 pdpID="6" 
                 type="TALONSRX" 
                 inverted="true" 
                 sensorInverted="false" 
                 feedbackDevice="NONE" 
                 countsPerRev="0" 
                 gearRatio="1.0" 
                 brakeMode="false" 
                 peakCurrentDuration="50" 
                 continuousCurrentLimit="50" 
                 peakCurrentLimit="50" 
                 currentLimiting="true"/>
     </mechanism>

<!-- ====================================================
     Arm
     ====================================================  -->
     <mechanism type= "ARM"
               networkTable="ArmNT"
               controlFile="arm.xml">
          <motor usage="ARM"
                 canId="10"
                 pdpID="10"
                 type="TALONSRX"
                 inverted="true"
                 sensorInverted="false"
                 feedbackDevice="NONE" 
                 countsPerRev="0" 
                 gearRatio="1.0" 
                 brakeMode="true" 
                 peakCurrentDuration="50" 
                 continuousCurrentLimit="50" 
                 peakCurrentLimit="50" 
                 currentLimiting="true"              
                 forwardlimitswitch="true" 
                 forwardlimitswitchopen="true" 
                 reverselimitswitch="true" 
                 reverselimitswitchopen="true"/> 

     </mechanism>

<!-- ====================================================
     Ball Release
     ====================================================  -->
     <mechanism type= "RELEASE"
                networkTable="BallReleaseNT"
                controlFile="release.xml">
          <servo usage="RELEASE_SERVO"
                 pwmId="0"
                 minAngle="0.0"
                 maxAngle="360.0"
                 pwmId="0"/>
          <servo usage="RELEASE_SERVO2"
                 pwmId="1"
                 minAngle="0.0"
                 maxAngle="360.0"
                 pwmId="1"/>
     </mechanism>

